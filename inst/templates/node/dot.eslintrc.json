{
  "parser": "@babel/eslint-parser",
  "extends": ["airbnb-base"],
  "env": {
    "browser": true
  },
  "globals": {
    "$": "readonly",
    "Shiny": "readonly"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-alert": "off",
    "no-console": "off"
  }
}
